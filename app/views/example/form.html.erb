

<%= form_for :JobApplication do |f| %>
  <p>
    <%= f.label :full_name %><br>
    <%= f.text_field :full_name %>
  </p>

  <p>
    <%= f.label :phone %><br>
    <%= f.phone_field :phone %>
  </p>

  <p>
    <%= f.label :hobby %><br>
    <%= f.text_area :hobby %>
  </p>

  <p>
    <%= f.label :years_experience %><br>
    <%= f.number_field :years_experience %>
  </p>

  <p>
    <%= f.label :available_date %><br>
    <%= f.date_field :available_date %>
  </p>

  <p>
    <%= f.submit "Submit" %>
  </p>
<% end %>
